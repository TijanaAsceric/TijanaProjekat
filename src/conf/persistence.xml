<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" 
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence  
            http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="napredne" transaction-type="JTA">
    <jta-data-source>napredne</jta-data-source>
    <class>domen.Administrator</class>
    <class>domen.Advokat</class>
    <class>domen.Klijent</class>
    <class>domen.Korisnik</class>
    <class>domen.Mesto</class>
    <class>domen.Predmet</class>
    <class>domen.Rociste</class>
    <class>domen.Stavkarocista</class>
    <class>domen.Sud</class>
    <class>domen.Usluga</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
